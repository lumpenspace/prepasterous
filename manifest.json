
{
  "manifest_version": 3,
  "name": "Paste Interceptor",
  "version": "1.0",
  "permissions": ["clipboardRead", "clipboardWrite","activeTab", "scripting", "storage", "notifications"],
  "background": {
    "service_worker": "dist/background.js"

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_title": "Paste Interceptor"
  }  
}
