
{
  "manifest_version": 3,
  "name": "Paste Interceptor",
  "version": "1.0",
  "permissions": ["clipboardRead","activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
